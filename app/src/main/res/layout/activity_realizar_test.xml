<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".ActivityRealizarTest">


    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbarMyToolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:titleTextColor="@android:color/primary_text_dark"
        android:background="@color/primaryColor"
        android:elevation="4dp"
        android:theme="@style/ThemeOverlay.AppCompat.ActionBar"
        app:popupTheme="@style/ThemeOverlay.AppCompat.Light"
        ></androidx.appcompat.widget.Toolbar>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/run_test_instructions"></TextView>


    <Button
        android:id="@+id/btnVerResultadoTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ver_resultados"></Button>

    <TextView
        android:id="@+id/tvTestUniqueId"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        ></TextView>



    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerResolverTest"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        ></androidx.fragment.app.FragmentContainerView>

    <ListView
        android:id="@+id/lvPreguntasTest"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">


    </ListView>


</LinearLayout>